{
  "name": "better-deepseek",
  "version": "0.1.0",
  "private": true,
  "description": "manifest.json description",
  "type": "module",
  "scripts": {
    "build": "wxt build && npm run rm:public",
    "build:firefox": "wxt build -b firefox && npm run rm:public",
    "compile": "tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "format": "biome check --write .",
    "postinstall": "igit install",
    "lint:all": "npm run format",
    "rm:public": "rm -rf .output/chrome-mv3/wxt.svg .output/chrome-mv3/config.json",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "html-to-md": "^0.8.7",
    "mermaid": "^11.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@doremijs/biome-config": "^0.1.5",
    "@doremijs/igit-cli": "^0.0.9-fix-1",
    "@types/node": "^22.13.15",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@wxt-dev/module-react": "^1.1.2",
    "typescript": "^5.6.3",
    "wxt": "^0.20.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
